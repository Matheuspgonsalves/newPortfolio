{
    "version": 2,
    "builds": [
        { "src": "/HTML/index.html", "use": "@vercel/static" },
        { "src": "CSS/**/*.css", "use": "@vercel/static" },
        { "src": "Js/**/*.js", "use": "@vercel/static" }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/HTML/index.html"
        }
    ]
}
